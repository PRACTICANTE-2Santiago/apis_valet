RewriteEngine off

RewriteRule ^api/administradores/?$ api/administradores.php
RewriteRule ^api/automovil/?$ api/automovil.php
RewriteRule ^api/choferes/?$ api/choferes.php
RewriteRule ^api/choferlugares/?$ api/choferlugares.php
RewriteRule ^api/comercios/?$ api/comercios.php
RewriteRule ^api/contador/?$ api/contador.php
RewriteRule ^api/creditos/?$ api/creditos.php
RewriteRule ^api/usuarios/?$ api/usuarios.php
RewriteRule ^api/valet/?$ api/valet.php
RewriteRule ^api/comercios/([0-9]+)?$ api/comercios.php?id_valet=$
RewriteRule ^api/valet/([0-9]+)?$ api/valet.php?id=$
RewriteRule ^api/valet/([0-9]+)?$ api/valet.php?id_pin=$
RewriteRule ^api/valet/([0-9]+)?$ api/valet.php?nombre=$
RewriteRule ^api/choferes/([0-9]+)?$ api/choferes.php?id_pin=$
RewriteRule ^api/choferes/([0-9]+)?$ api/choferes.php?id=$&token=$

